#include <iostream>
#include <string>
using namespace std;
int main()
{
    int a;
    cin >> a;
    while (a--)
    {
        char b;
        cin >> b;
        if (b >= 'A' && b <= 'Z')
        {
            cout << static_cast<char>(b + 32) << endl;
        }
        else
        {
            cout << static_cast<char>(b - 32) << endl;
        }
    }
}
