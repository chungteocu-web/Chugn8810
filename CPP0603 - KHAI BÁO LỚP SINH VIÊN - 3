#include <iostream>
#include <iomanip>
#include <vector>
#include <string>
#include <sstream>
using namespace std;
struct SinhVien
{
    string masv = "B20DCCN001", ten, lop, ngaysinh;
    double GPA;
    void chuanhoa()
    {
        vector<string> xau;
        string tmp, x;
        for (int i = 0; i < ten.size(); i++)
        {
            ten[i] = tolower(ten[i]);
        }
        stringstream ss(ten);
        while (ss >> tmp)
        {
            xau.push_back(tmp);
        }
        ten.clear();
        for (int i = 0; i < xau.size(); i++)
        {
            xau[i][0] = toupper(xau[i][0]);
            ten += xau[i];
            ten += " ";
        }
    }
    friend istream &operator>>(istream &vao, SinhVien &sv)
    {

        getline(vao, sv.ten);
        vao >> sv.lop >> sv.ngaysinh >> sv.GPA;
        if (sv.ngaysinh[1] == '/')
        {
            sv.ngaysinh.insert(0, 1, '0');
        }
        if (sv.ngaysinh[4] == '/')
        {
            sv.ngaysinh.insert(3, 1, '0');
        }
        sv.chuanhoa();
        return vao;
    }
    friend ostream &operator<<(ostream &xuat, const SinhVien &sv)
    {
        xuat << sv.masv << " " << sv.ten << " " << sv.lop << " " << sv.ngaysinh << " " << fixed << setprecision(2) << sv.GPA << endl;
        return xuat;
    }
};

int main(){
    SinhVien a;
    cin >> a;
    cout << a;
    return 0;
}
