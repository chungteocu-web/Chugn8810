#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;
int uocnt(int m)
{
    if (m < 2)
        return 1;
    for (int i = 2; i * i <= m; i++)
    {
        if (m % i == 0)
        {
            return i;
        }
    }
    return m;
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int a;
        cin >> a;
        for (int i = 1; i <= a; i++)
        {
            int tmp = uocnt(i);
            cout << tmp << " ";
        }
        cout << endl;
    }
}
