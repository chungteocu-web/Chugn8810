#include <iostream>
#include <iomanip>
#include <algorithm>
#include <vector>
#include <string>
#include <sstream>
using namespace std;
int check = 1;
class SinhVien
{
private:
    string ma, ten, lop, ngaysinh;
    double GPA;

public:
    void chuanhoa()
    {
        for (int i = 0; i < ten.size(); i++)
        {
            ten[i] = tolower(ten[i]);
        }
        vector<string> xau;
        string tmp;
        stringstream ss(ten);
        while (ss >> tmp)
        {
            xau.push_back(tmp);
        }
        ten.clear();
        for (int i = 0; i < xau.size(); i++)
        {
            xau[i][0] = toupper(xau[i][0]);
            ten += xau[i];
            if (i < ten.size() - 1)
            {
                ten += " ";
            }
        }
    }
    friend istream &operator>>(istream &in, SinhVien &a);
    friend ostream &operator<<(ostream &out, SinhVien &a);
};

istream &operator>>(istream &in, SinhVien &a)
{
    string s;
    if (check < 10)
    {
        s = "B20DCCN00";
    }
    else
    {
        s = "B20DCCN0";
    }
    s += to_string(check++);
    a.ma = s;
    in.ignore();
    getline(in, a.ten);
    in >> a.lop >> a.ngaysinh >> a.GPA;
    return in;
}
ostream &operator<<(ostream &out, SinhVien &a)
{
    if (a.ngaysinh[1] == '/')
        a.ngaysinh.insert(0, 1, '0');
    if (a.ngaysinh[4] == '/')
        a.ngaysinh.insert(3, 1, '0');
    a.chuanhoa();
    out << a.ma << " " << a.ten << " " << a.lop << " " << a.ngaysinh << " " << fixed << setprecision(2) << a.GPA << endl;
    return out;
}
int main(){
    SinhVien ds[50];
    int N, i;
    cin >> N;
    for(i=0;i<N;i++){
        cin >> ds[i];
    }
    for(i=0;i<N;i++){
        cout << ds[i];
    }
    return 0;
}
