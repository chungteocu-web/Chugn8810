#include <iostream>
#include <string>
#include <map>
#include <algorithm>
#include <vector>
using namespace std;
int main()
{
    int k;
    cin >> k;
    while (k--)
    {
        int m, n, x, max = 0;
        cin >> m >> n;
        map<int, int> mp;
        for (int i = 0; i < m; i++)
        {
            cin >> x;
            mp[x]++;
        }
        if (mp[n] == 0)
            cout << -1 << endl;
        else
        {
            cout << mp[n] << endl;
        }
    }
}
