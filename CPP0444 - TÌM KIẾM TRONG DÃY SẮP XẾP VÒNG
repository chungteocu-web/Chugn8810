#include <iostream>
#include <string>
#include <map>
#include <vector>
using namespace std;
int main()
{
    int k;
    cin >> k;
    while (k--)
    {
        int m, n;
        cin >> m >> n;
        vector<int> a(m);
        for (int i = 0; i < m; i++)
        {
            cin >> a[i];
        }
        for (int i = 1; i < a.size(); i++)
        {
            if (n == a[i])
            {
                cout << i + 1 << endl;
            }
        }
    }
}
