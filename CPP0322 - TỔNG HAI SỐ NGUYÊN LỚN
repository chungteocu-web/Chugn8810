#include <iostream>
#include <algorithm>
#include <set>
#include <string>
#include <sstream>
using namespace std;
string cong(string a, string b)
{
    while (a.size() > b.size())
    {
        b = "0" + b;
    }
    while (a.size() < b.size())
    {
        a = "0" + a;
    }
    string x;
    int nho = 0;
    for (int i = a.size() - 1; i >= 0; i--)
    {
        int so1 = a[i] - '0', so2 = b[i] - '0';
        int sum = so1 + so2 + nho;
        x += ((sum % 10) + '0');
        nho = sum / 10;
    }
    if (nho > 0)
    {
        x += "1";
    }
    reverse(x.begin(), x.end());
    return x;
}
int main()
{
    int m;
    cin >> m;
    cin.ignore();
    while (m--)
    {
        string s1, s2, tmp;
        cin >> s1 >> s2;
        string tmpmax = cong(s1, s2);
        cout << tmpmax << endl;
    }
}
