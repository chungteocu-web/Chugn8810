#include <iostream>
#include <vector>
#include <algorithm>
#include <map>
using namespace std;
int main()
{
    int m;
    cin >> m;
    cin.ignore();
    while (m--)
    {
        int k, n;
        cin >> k >> n;
        int a[k + n];
        for (int i = 0; i < n + k; i++)
        {
            cin >> a[i];
        }
        sort(a, a + (k + n));
        for (int i = 0; i < k + n; i++)
        {
            cout << a[i] << " ";
        }
        cout << endl;
    }
}
