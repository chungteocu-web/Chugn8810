#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;
int prime(int n)
{
    if (n == 2 || n == 3)
    {
        return 1;
    }
    if (n < 3 || n % 2 == 0 || n % 3 == 0)
    {
        return 0;
    }
    for (int i = 5; i * i <= n; i += 6)
    {
        if (n % i == 0 || n % (i + 2) == 0)
        {
            return 0;
        }
    }
    return 1;
}
int main()
{
    int c;
    cin >> c;
    while (c--)
    {
        int a, check = 0;
        cin >> a;
        for (int i = 2; i < a; i++)
        {
            if (prime(i))
            {
                if (prime(a - i))
                {
                    cout << i << " " << a - i;
                    ++check;
                    break;
                }
            }
        }
        if (check == 0)
            cout << -1;
        cout << endl;
    }
}
