#include <iostream>
#include <iomanip>
#include <algorithm>
#include <vector>
#include <string>
#include <sstream>
using namespace std;
struct SinhVien
{
    string ten, ngaysinh;
    int nam, thang, ngay;
};
bool sosanh(SinhVien a, SinhVien b)
{
    if (a.nam > b.nam)
        return 1;
    if (a.nam == b.nam)
    {
        if (a.thang > b.thang)
            return 1;
        if (a.thang == b.thang)
        {
            if (a.ngay > b.ngay)
                return 1;
        }
    }
    return 0;
}
void nhap(SinhVien &a)
{
    cin >> a.ten;
    cin >> a.ngaysinh;
    a.nam = stoi(a.ngaysinh.substr(6));
    a.thang = stoi(a.ngaysinh.substr(3, 2));
    a.ngay = stoi(a.ngaysinh.substr(0, 2));
}
int main()
{
    int n;
    cin >> n;
    struct SinhVien ds[n];
    for (int i = 0; i < n; i++)
    {
        nhap(ds[i]);
    }
    sort(ds, ds + n, sosanh);
    cout << ds[0].ten << endl
         << ds[n - 1].ten << endl;
    return 0;
}
