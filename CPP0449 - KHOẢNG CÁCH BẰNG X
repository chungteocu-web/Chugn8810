#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;
int binary(vector<int> &a, int x)
{
    int l = 0, r = a.size() - 1;
    while (l <= r)
    {
        int mid = (l + r) / 2;
        if (a[mid] == x)
            return 1;
        if (a[mid] < x)
        {
            l = mid + 1;
        }
        if (a[mid] > x)
        {
            r = mid - 1;
        }
    }
    return 0;
}
int main()
{
    int m, n, x;
    cin >> m;
    while (m--)
    {
        cin >> n >> x;
        vector<int> a(n);
        for (int i = 0; i < n; i++)
        {
            cin >> a[i];
        }
        int check = 0;
        sort(a.begin(), a.end());
        for (int i = 0; i < n; i++)
        {
            int tmp = x + a[i];
            if (binary(a, tmp))
            {
                check = 1;
                break;
            }
        }
        if (check == 1)
        {
            cout << 1;
        }
        else
            cout << -1;

        cout << endl;
    }
}
