#include <iostream>
#include <algorithm>
#include <vector>
#include <cmath>
using namespace std;
int main()
{
    int m, n, k;
    cin >> m;
    while (m--)
    {
        int phepdoi = 0;
        cin >> n >> k;
        vector<int> a(n), b(n);
        for (int i = 0; i < n; i++)
        {
            cin >> a[i];
        }
        int sobangk = 0;
        for (int i = 0; i < n; i++)
        {
            if (a[i] <= k)
            {
                ++sobangk;
            }
        }
        int ans = 0;
        for (int i = 0; i < sobangk; i++)
        {
            if (a[i] <= k)
            {
                ++ans;
            }
        }
        int max = ans;
        for (int i = sobangk; i < n; i++)
        {
            if (a[i - sobangk] <= k)
            {
                --ans;
            }
            if (a[i] <= k)
            {
                ++ans;
            }
            if (max < ans)
            {
                max = ans;
            }
        }
        cout << sobangk - max << endl;
    }
}
