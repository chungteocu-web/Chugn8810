#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;
int check(long long m)
{
    if (m == 2 || m == 3)
    {
        return 1;
    }
    if (m < 3 || m % 2 == 0 || m % 3 == 0)
    {
        return 0;
    }
    for (int i = 5; i * i <= m; i += 6)
    {
        if (m % i == 0 || m % (i + 2) == 0)
        {
            return 0;
        }
    }
    return 1;
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        long long a, b, count = 0;
        cin >> a >> b;
        for (long long i = sqrt(a); i <= sqrt(b); i++)
        {
            if (check(i))
            {
                ++count;
            }
        }
        cout << count << endl;
    }
}
