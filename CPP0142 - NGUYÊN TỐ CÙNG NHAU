#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;
int prime(int n)
{
    if (n == 2 || n == 3)
    {
        return 1;
    }
    if (n < 3 || n % 2 == 0 || n % 3 == 0)
    {
        return 0;
    }
    for (int i = 5; i * i <= n; i += 6)
    {
        if (n % i == 0 || n % (i + 2) == 0)
        {
            return 0;
        }
    }
    return 1;
}
int ucln(int a, int b)
{
    while (b != 0)
    {
        int tmp = a % b;
        a = b;
        b = tmp;
    }
    if (a == 1)
    {
        return 1;
    }
    return 0;
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int a, sum = 0;
        cin >> a;
        for (int i = 1; i <= a; i++)
        {
            if (ucln(i, a))
            {
                ++sum;
            }
        }
        if (prime(sum))
            cout << 1 << endl;
        else
            cout << 0 << endl;
    }
}
