#include <iostream>
#include <vector>
#include <algorithm>
#include <map>
using namespace std;
int main()
{
    int m;
    cin >> m;
    cin.ignore();
    while (m--)
    {
        map<int, int> mp;
        int k, n, sum = 0, max = 0, min = 99999999;
        cin >> k;
        vector<int> a(k);
        for (int i = 0; i < k; i++)
        {
            cin >> n;
            if (n > max)
            {
                max = n;
            }
            if (n < min)
            {
                min = n;
            }
            mp[n] = 1;
        }
        for (int i = min; i <= max; i++)
        {
            if (mp[i] != 1)
            {
                ++sum;
            }
        }
        cout << sum << endl;
    }
}
