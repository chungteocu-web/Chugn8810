#include <iostream>
#include <cmath>
#include <algorithm>
#include <vector>
using namespace std;
int main()
{
    int a;
    cin >> a;
    int sum = pow(a, 2);
    vector<int> A(sum);
    int res[a][a];
    for (int i = 0; i < sum; i++)
    {
        cin >> A[i];
    }
    sort(A.begin(), A.end());
    int h1 = 0, h2 = a - 1, c1 = 0, c2 = a - 1, p = 0;
    while (h1 <= h2 && c1 <= c2)
    {
        for (int i = c1; i <= c2; i++)
        {
            res[h1][i] = A[p++];
        }
        h1++;
        for (int i = h1; i <= h2; i++)
        {
            res[i][c2] = A[p++];
        }
        c2--;
        if (h1 <= h2)
        {
            for (int i = c2; i >= c1; i--)
            {
                res[h2][i] = A[p++];
            }
        }
        if (c1 <= c2)
        {
            h2--;
            for (int i = h2; i >= h1; i--)
            {
                res[i][c1] = A[p++];
            }
            c1++;
        }
    }
    for (int i = 0; i < a; i++)
    {
        for (int j = 0; j < a; j++)
        {
            cout << res[i][j] << " ";
        }
        cout << endl;
    }
    cout << endl;
}
