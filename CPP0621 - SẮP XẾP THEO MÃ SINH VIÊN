#include <iostream>
#include <iomanip>
#include <algorithm>
#include <vector>
#include <string>
#include <sstream>
using namespace std;
class SinhVien
{
private:
    string masv, ten, lop, email;

public:
    friend istream &operator>>(istream &in, SinhVien &a)
    {
        if (!(in >> ws))
            return in;
        getline(in, a.masv);
        getline(in, a.ten);
        getline(in, a.lop);
        getline(in, a.email);
        return in;
    }
    friend ostream &operator<<(ostream &out, const SinhVien &a)
    {
        out << a.masv << " " << a.ten << " " << a.lop << " " << a.email << endl;
        return out;
    }
    friend bool cmp(const SinhVien &a, const SinhVien &b);
};
bool cmp(const SinhVien &a, const SinhVien &b)
{
    return a.masv < b.masv;
}
int main()
{
    vector<SinhVien> ds;
    SinhVien tmp;
    while (cin >> tmp)
    {
        ds.push_back(tmp);
    }
    sort(ds.begin(), ds.end(), cmp);
    for (int i = 0; i < ds.size(); i++)
        cout << ds[i];
}
