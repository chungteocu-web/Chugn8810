#include <iostream>
#include <iomanip>
#include <algorithm>
#include <vector>
#include <string>
#include <sstream>
using namespace std;
int check = 1;
class NhanVien
{
private:
    string stt, ten, sex, ngaysinh, que, ma, ngay;

public:
    friend istream &operator>>(istream &in, NhanVien &a);
    friend ostream &operator<<(ostream &out, NhanVien &b);
};
istream &operator>>(istream &in, NhanVien &a)
{
    in.ignore();
    string s;
    if (check < 10)
        s += "0000";
    else
        s += "000";
    s += to_string(check++);
    a.stt = s;
    getline(in, a.ten);
    getline(in, a.sex);
    getline(in, a.ngaysinh);
    getline(in, a.que);
    getline(in, a.ma);
    in >> a.ngay;
    return in;
}
ostream &operator<<(ostream &out, NhanVien &b)
{
    out << b.stt << " " << b.ten << " " << b.sex << " " << b.ngaysinh << " " << b.que << " " << b.ma << " " << b.ngay << endl;
    return out;
}
int main(){
    NhanVien ds[50];
    int N,i;
    cin >> N;
    for(i=0;i<N;i++) cin >> ds[i];
    for(i=0;i<N;i++) cout << ds[i];
    return 0;
}
