#include <iostream>
#include <iomanip>
#include <algorithm>
#include <vector>
#include <string>
#include <sstream>
using namespace std;
class SinhVien
{
public:
    string masv, ten, lop, email, nam;
    friend istream &operator>>(istream &in, SinhVien &a)
    {
        in >> ws;
        getline(in, a.masv);
        getline(in, a.ten);
        getline(in, a.lop);
        getline(in, a.email);
        a.nam = a.lop.substr(1, 2);
        return in;
    }
    friend ostream &operator<<(ostream &out, const SinhVien &a)
    {
        out << a.masv << " " << a.ten << " " << a.lop << " " << a.email << endl;
        return out;
    }
    friend bool cmp(const SinhVien &a, const SinhVien &b);
};
bool cmp(const SinhVien &a, const SinhVien &b)
{
    return a.masv < b.masv;
}
int main()
{
    int n, m;
    string s, tmp;
    cin >> n;
    SinhVien ds[n];
    for (int i = 0; i < n; i++)
    {
        cin >> ds[i];
    }
    cin >> m;
    while (m--)
    {
        cin >> s;
        tmp = s.substr(2, 2);
        cout << "DANH SACH SINH VIEN KHOA " << s << ":" << endl;
        for (int i = 0; i < n; i++)
        {
            if (ds[i].nam == tmp)
            {
                cout << ds[i];
            }
        }
    }
}
